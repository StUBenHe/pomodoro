{
  "manifest_version": 3,
  "name": "Pomodoro Timer",
  "version": "1.1",
  "description": "一个简单的番茄钟 Chrome 插件",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "bud.png",
      "48": "bud.png",
      "128": "bud.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
"permissions": ["alarms", "storage", "notifications"],
"host_permissions": ["<all_urls>"]

}
